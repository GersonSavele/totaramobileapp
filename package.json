  "config": {
    "maxPackagesNumber": 100,
    "maxSizeBites": 840400,
    "allowedLicenseTypes": [
      "weaklyProtective",
      "protective",
      "permissive",
      "publicDomain",
      "uncategorized"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/TotaraMobileApp.app",
        "build": "xcodebuild -workspace ios/TotaraMobileApp.xcworkspace -scheme TotaraMobileApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      }
    },
    "test-runner": "jest"
