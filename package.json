{
  "name": "TotaraMobileApp",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --configuration Dev.Debug --scheme TotaraMobileApp",
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "local:android-release-apk": "cd android/ && ./gradlew installProdRelease",
    "local:android-release-aab": "cd android/ && ./gradlew bundleProdRelease",
    "load:env_test": "ENVFILE=.env.test",
    "ci:ios-test": "react-native run-ios --configuration Test.Debug",
    "ci:android-test": "react-native run-android --variant=qaDebug --appIdSuffix=qa",
    "local:ios-test": "npm-run-all load:env_test ci:ios-test",
    "local:android-test": "npm-run-all load:env_test ci:android-test",
    "android_release": "react-native run-android --variant release",
    "build_scorm": "webpack --config ./TotaraMobileOfflineScorm/webpack.config.js",
    "debug": "REACT_DEBUGGER=\"open -g 'rndebugger://set-debugger-loc?port=8001' ||\" react-native start",
    "start": "react-native start",
    "test": "jest",
    "test-coverage": "jest --config jest.config.js --collectCoverage",
    "test-junit": "jest --config jest.config.js --ci --reporters=jest-junit --reporters=default",
    "eslint": "eslint -c .eslintrc.json --ext .js --ext .ts --ext .tsx ./src/totara ./TotaraMobileOfflineScorm",
    "clean": "rm -rf node_modules",
    "reconnect": "adb reverse tcp:8081 tcp:8081 && yarn refresh",
    "menu": "adb shell input keyevent 82",
    "refresh": "adb shell input text \"RR\"",
    "xcode": "open ios/TotaraMobileApp.xcworkspace",
    "clear_cache:temp": "rm -rf $TMPDIR/react-*",
    "clear_cache:watchman": "watchman watch-del-all",
    "clear_cache:ios": "rm -rf ios/build/ModuleCache/*",
    "clear_cache:npm": "npm cache clean --force",
    "clear:modules": "rm -rf node_modules",
    "clean:pods": "rm -rf ios/Pods/ && cd ios && pod install && cd ..",
    "clear:cache": "npm-run-all --parallel clear_cache:*",
    "clear:all": "npm-run-all clear:cache clear:modules && yarn && yarn clean:pods",
    "clear:jest": "npx jest --clearCache",
    "install:translations": "node scripts/post-install/install-translations.js",
    "jetify": "npx jetify",
    "mock:schema:setup": "get-graphql-schema https://mobile.demo.totara.software/totara/webapi/dev_graphql_executor.php --json > e2e/graphql/schema.json",
    "postinstall": "npm-run-all install:translations",
    "mock:setup": "mv src/totara/lib/config.local.ts src/totara/lib/config.local.bak.ts &&  cp src/totara/lib/config.detox.ts src/totara/lib/config.local.ts",
    "mock:reset": "mv src/totara/lib/config.local.bak.ts src/totara/lib/config.local.ts",
    "detox:test": "detox build && detox test || echo '`detox test` completed.'",
    "detox:run": "npm-run-all mock:setup detox:test mock:reset",
    "devices": "npm-run-all devices:ios devices:android",
    "devices:ios": "xcrun xctrace list devices",
    "devices:android": "adb devices",
    "test:debug": "node --inspect-brk --inspect ./node_modules/.bin/jest -i",
    "test:memory": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage"
  },
  "resolutions": {
    "@codler/react-native-keyboard-aware-scroll-view": "2.0.1"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@codler/react-native-keyboard-aware-scroll-view": "^2.0.1",
    "@dr.pogodin/react-native-static-server": "github:birdofpreyru/react-native-static-server",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-native-fontawesome": "^0.2.3",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-native-cookies/cookies": "github:react-native-cookies/cookies",
    "@react-native-firebase/app": "^14.5.0",
    "@react-native-firebase/messaging": "^14.5.0",
    "@react-navigation/compat": "^5.3.10",
    "@react-navigation/material-bottom-tabs": "^5.3.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@sentry/react-native": "^3.2.5",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-timeout": "^4.0.0",
    "apollo3-cache-persist": "^0.9.1",
    "graphql": "^15.0.0",
    "i18n-js": "^3.3.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "native-base": "^2.13.14",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-native": "0.67.5",
    "react-native-animatable": "^1.3.3",
    "react-native-config": "^1.4.1",
    "react-native-device-info": "^5.5.4",
    "react-native-elements": "^3.4.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.3.4",
    "react-native-orientation-locker": "^1.4.0",
    "react-native-paper": "^4.7.1",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.18.2",
    "react-native-sensitive-info": "https://github.com/mCodex/react-native-sensitive-info.git#keystore",
    "react-native-skeleton-placeholder": "^4.0.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-view-more-text": "^2.1.0",
    "react-native-view-pdf": "^0.11.0",
    "react-native-webview": "^11.17.2",
    "react-native-zip-archive": "^6.0.6",
    "react-redux": "^7.2.0",
    "reanimated-bottom-sheet": "1.0.0-alpha.20",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "xmldom": "^0.3.0",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@testing-library/react-hooks": "^2.0.1",
    "@testing-library/react-native": "^7.1.0",
    "@types/i18n-js": "^3.0.1",
    "@types/jasmine": "^3.6.1",
    "@types/jest": "^27.4.0",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.66.15",
    "@types/react-native-snap-carousel": "^3.7.4",
    "@types/react-native-video": "^5.0.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.5.1",
    "detox": "^17.11.4",
    "download": "^8.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^8.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4",
    "unzipper": "^0.10.11",
    "waait": "^1.0.5",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "config": {
    "maxPackagesNumber": 100,
    "maxSizeBites": 840400,
    "allowedLicenseTypes": [
      "weaklyProtective",
      "protective",
      "permissive",
      "publicDomain",
      "uncategorized"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Totara.app",
        "build": "xcodebuild -workspace ios/TotaraMobileApp.xcworkspace -scheme TotaraMobileApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      }
    },
    "test-runner": "jest"
  }
}
