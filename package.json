{
  "name": "TotaraMobileApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --configuration Dev.Debug --scheme TotaraMobileApp",
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "local:android-release": "cd android/ && ./gradlew installProdRelease",
    "load:env_test": "ENVFILE=.env.test",
    "ci:ios-test": "react-native run-ios --configuration Test.Release",
    "ci:android-test": "react-native run-android --variant=testingRelease --appIdSuffix=test",
    "local:ios-test": "npm-run-all load:env_test ci:ios-test",
    "local:android-test": "npm-run-all load:env_test ci:android-test",
    "android_release": "react-native run-android --variant release",
    "build_scorm": "webpack --config ./TotaraMobileOfflineScorm/webpack.config.js",
    "debug": "REACT_DEBUGGER=\"open -g 'rndebugger://set-debugger-loc?port=8001' ||\" react-native start",
    "start": "react-native start",
    "test": "jest",
    "test-coverage": "jest --config jest.config.js --collectCoverage",
    "test-junit": "jest --config jest.config.js --ci --reporters=jest-junit --reporters=default",
    "eslint": "eslint -c .eslintrc.json --ext .js --ext .ts --ext .tsx ./src/totara ./TotaraMobileOfflineScorm",
    "clean": "rm -rf node_modules",
    "reconnect": "adb reverse tcp:8081 tcp:8081 && yarn refresh",
    "menu": "adb shell input keyevent 82",
    "refresh": "adb shell input text \"RR\"",
    "xcode": "open ios/TotaraMobileApp.xcworkspace",
    "clear_cache:temp": "rm -rf $TMPDIR/react-*",
    "clear_cache:watchman": "watchman watch-del-all",
    "clear_cache:ios": "rm -rf ios/build/ModuleCache/*",
    "clear_cache:npm": "npm cache clean --force",
    "clear:modules": "rm -rf node_modules",
    "clean:pods": "rm -rf ios/Pods/ && cd ios && pod install && cd ..",
    "clear:cache": "npm-run-all --parallel clear_cache:*",
    "clear:all": "npm-run-all clear:cache clear:modules && npm i && yarn clean:pods",
    "clear:jest": "npx jest --clearCache",
    "install:translations": "node scripts/post-install/install-translations.js",
    "jetify": "npx jetify",
    "fixIOS14images": "run-script-os",
    "fixIOS14images:macos": "npx react-native-fix-image",
    "fixIOS14images:windows": "",
    "postinstall": "npm-run-all jetify install:translations fixIOS14images"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-native-fontawesome": "^0.2.3",
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/cookies": "^2.0.7",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.4",
    "@react-native-firebase/app": "^6.7.1",
    "@react-native-firebase/messaging": "^6.7.1",
    "@react-navigation/compat": "^5.3.10",
    "@react-navigation/material-bottom-tabs": "^5.3.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@sentry/react-native": "^1.3.9",
    "apollo-boost": "^0.4.4",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link-context": "^1.0.19",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-retry": "^2.2.15",
    "apollo-link-timeout": "^1.4.1",
    "graphql": "^15.0.0",
    "i18n-js": "^3.3.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "native-base": "^2.13.14",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-apollo": "3.1.3",
    "react-native": "0.62.2",
    "react-native-animatable": "^1.3.3",
    "react-native-config": "^1.4.1",
    "react-native-device-info": "^5.5.4",
    "react-native-fast-image": "^8.3.2",
    "react-native-fix-image": "^2.0.1",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-localize": "^1.3.4",
    "react-native-orientation-locker": "^1.2.0",
    "react-native-paper": "^4.4.0",
    "react-native-progress": "^4.1.0",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.4.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-static-server": "^0.4.2",
    "react-native-svg": "^12.0.3",
    "react-native-vector-icons": "^6.6.0",
    "react-native-view-more-text": "^2.1.0",
    "react-native-view-pdf": "^0.11.0",
    "react-native-webview": "^10.7.0",
    "react-native-zip-archive": "^5.0.1",
    "react-redux": "^7.2.0",
    "reanimated-bottom-sheet": "1.0.0-alpha.20",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "xmldom": "^0.3.0",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.3",
    "@babel/core": "^7.9.2",
    "@babel/runtime": "^7.9.2",
    "@testing-library/react-hooks": "^2.0.1",
    "@testing-library/react-native": "^7.1.0",
    "@types/i18n-js": "^3.0.1",
    "@types/jasmine": "^3.6.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.17",
    "@types/react-native-snap-carousel": "^3.7.4",
    "@types/react-native-video": "^5.0.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.0",
    "@types/recompose": "^0.30.7",
    "@types/redux-logger": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-jest": "^25.5.1",
    "detox": "^17.11.4",
    "download": "^8.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.1",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^8.0.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.54.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "run-script-os": "^1.1.5",
    "ts-jest": "^25.5.1",
    "typescript": "^3.6.3",
    "unzipper": "^0.10.11",
    "waait": "^1.0.5",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "config": {
    "maxPackagesNumber": 100,
    "maxSizeBites": 840400,
    "allowedLicenseTypes": [
      "weaklyProtective",
      "protective",
      "permissive",
      "publicDomain",
      "uncategorized"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Totara.app",
        "build": "xcodebuild -workspace ios/TotaraMobileApp.xcworkspace -scheme TotaraMobileApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      }
    },
    "test-runner": "jest"
  }
}
