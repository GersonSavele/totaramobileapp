{
  "name": "TotaraMobileApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "prebuild": "npx expo prebuild --clean",
    "test": "npx jest --watchAll",
    "test:ci": "npx jest --ci",
    "lint": "npx eslint . --quiet --ext .js,.jsx,.ts,.tsx",
    "translations": "node scripts/post-install/install-translations.js",
    "lint:types": "npx tsc  --noemit",
    "eas:build:dev": "npx eas build --platform android --profile development --non-interactive",
    "eas:build:uat": "npx eas build --platform all --branch uat",
    "eas:build:prod": "npx eas build --platform all --branch prod",
    "eas-build-pre-install": "./scripts/eas/eas-build-pre-install.sh",
    "eas-build-on-success": "sh ./scripts/eas/eas-build-on-success.sh",
    "eas-build-on-error": "sh ./scripts/eas/eas-build-on-error.sh"
  },
