{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noImplicitAny": false,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@placeholders/*": [
        "./src/placeholders/*"
      ],
      "@totara/*": [
        "./src/totara/*"
      ],
      "@resources/*": [
        "./src/resources/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "e2e"
  ]
}
